{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "^h$"
      }
    ],
    "react/prop-types": "off"
  },
  "settings": {
    "react": {
      "pragma": "h"
    }
  }
}
